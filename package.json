{
  "name": "tween24",
  "version": "1.5.0",
  "description": "Tween24.js is animation library that enables fast coding using method chains.",
  "main": "./dist/tween24.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/tween24.esm.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/tween24.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/tween24.js"
      }
    }
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "dev": "webpack --mode development",
    "build": "webpack --mode production --config webpack.config.js && webpack --config webpack-esm.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://a24@github.com/a24/tween24js.git"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "CSS",
    "Tween",
    "Animation"
  ],
  "author": "Atsushi Kaga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a24/tween24js/issues"
  },
  "homepage": "https://github.com/a24/tween24js#readme",
  "dependencies": {},
  "devDependencies": {
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4"
  }
}
